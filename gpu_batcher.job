#!/bin/bash
#SBATCH --job-name=unique_k_only
#SBATCH -M ukko
#SBATCH -p gpu,gpu-long
#SBATCH -o unique_k_only.txt
#SBATCH -c 2
#SBATCH -t 23:59:00
#SBATCH --mem-per-cpu=64G
#SBATCH --gres=gpu:1
#SBATCH --array=1-4

module purge
module load CUDA
module load cuDNN
conda activate tpnica

srun --unbuffered python main.py --headless -D1 --num-data 1024 --data-seed $SLURM_ARRAY_TASK_ID
